    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.0.11",
    "benchmark": "^2.1.4",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "file-loader": "^0.11.1",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^2.1.1",
